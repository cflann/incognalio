{
  "name": "incognalio",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "The future of web browsing",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "options_page": "src/options/index.html",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action"
  },
  "permissions": [
    "<all_urls>",
    "tabs"
  ]
}
